{
  "name": "typeorm-test-db",
  "version": "1.0.2",
  "description": "Automated transactional lifecycle helpers for TypeORM tests",
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "prebuild": "pnpm clean",
    "test": "jest --ci --maxWorkers=50% --verbose --showSeed --coverage --config ./tests/jest.config.ts",
    "test:ci": "pnpm test",
    "docker": "docker compose up -d",
    "lint": "eslint --fix",
    "release": "standard-version",
    "test:mysql": "cross-env TEST_DB_TYPE=mysql pnpm test:ci",
    "test:mariadb": "cross-env TEST_DB_TYPE=mariadb pnpm test:ci",
    "test:postgres": "cross-env TEST_DB_TYPE=postgres pnpm test:ci",
    "test:sqlite": "cross-env TEST_DB_TYPE=sqlite pnpm test:ci",
    "test:better-sqlite3": "pnpm -s shx mkdir -p .tmp/better-sqlite && cross-env TEST_DB_TYPE=better-sqlite3 pnpm test:ci",
    "test:all": "pnpm test:mysql && pnpm test:mariadb && pnpm test:postgres && pnpm test:sqlite && pnpm test:better-sqlite3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanjoGonDev/typeorm-test-db.git"
  },
  "keywords": [
    "typeorm",
    "testing",
    "transactions",
    "jest"
  ],
  "author": "Juanjo GonzÃ¡lez (https://github.com/juanjoGonDev)",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@jest/test-result": "^30.2.0",
    "@jest/test-sequencer": "^30.1.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.11.11",
    "@types/seedrandom": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "better-sqlite3": "^9.4.3",
    "cross-env": "7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^30.1.3",
    "mysql2": "^3.11.2",
    "pg": "^8.11.5",
    "rimraf": "^5.0.5",
    "seedrandom": "^3.0.5",
    "shx": "^0.4.0",
    "sqlite3": "^5.1.7",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typeorm": "0.3.27",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.4",
  "volta": {
    "node": "18.20.8",
    "pnpm": "8.15.4"
  }
}
